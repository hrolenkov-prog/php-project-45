{
  "name": "hexlet/code",
  "description": "Brain Games",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "wp-cli/php-cli-tools": "^0.11"
  },
    "autoload": {
      "psr-4": {
        "Hexlet\\Code\\": "src/"
      },
      "files": [
        "src/Cli.php",
        "src/Engine.php",
        "src/Games/Even.php",
        "src/Games/Calc.php",
        "src/Games/Gcd.php",
        "src/Games/Progression.php",
        "src/Games/Prime.php"
      ]
    },
    "bin": [
      "bin/brain-games",
      "bin/brain-even",
      "bin/brain-calc",
      "bin/brain-gcd",
      "bin/brain-progression",
      "bin/brain-prime"
    ],
  "require-dev": {
    "squizlabs/php_codesniffer": "^4.0"
  }
}
